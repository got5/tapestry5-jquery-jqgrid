			<t:block id="block1">
			<!-- Core Components and features -->
			<ul>
        		<li>
            		<t:pagelink t:page="GROUPING_SimpleGroupingWithArrayData">Simple Grouping</t:pagelink>
        		</li>
        		<li>
            		<t:pagelink t:page="GROUPING_HideGroupingColumns">Hide Grouping Columns</t:pagelink>
        		</li>
        		<li>
            		<t:pagelink t:page="GROUPING_GroupedHeaders">Grouped Headers</t:pagelink>
        		</li>
        		<li>
            		<t:pagelink t:page="GROUPING_RtlSupport">RTL Support</t:pagelink>
        		</li>
        		<li>
            		<t:pagelink t:page="GROUPING_GroupingRowsCollapsed">Grouping Rows Collapsed</t:pagelink>
        		</li>
				<li>
            		<t:pagelink t:page="GROUPING_ChangeGrouping">Dynamically Change Grouping</t:pagelink>
        		</li>        	
        	</ul>
			</t:block>
			
			<t:block id="block2">
				<ul>
					<li>
	            		<t:pagelink t:page="BASIC_ColumnReordering">Column Reordering</t:pagelink>
	        		</li>
	        		<li>
	            		<t:pagelink t:page="BASIC_ColumnChooser">Column Chooser</t:pagelink>
	        		</li>
	        		<li>
	            		<t:pagelink t:page="SortableRows">Sortable Rows</t:pagelink>
	        		</li>
	        		<li>
	            		<t:pagelink t:page="ResizingGrid">Resizing Grid</t:pagelink>
	        		</li>
	        	</ul>
			</t:block>
		
